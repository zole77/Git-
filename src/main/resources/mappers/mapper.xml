<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mycom.ussum.repository.Repository">
    <select id="selectAllMember" resultType="com.mycom.ussum.vo.MemberVO">
        <![CDATA[
            SELECT *
            FROM "member_table"
        ]]>
    </select>

    <select id="selectOneMember" resultType="com.mycom.ussum.vo.MemberVO" parameterType="com.mycom.ussum.vo.MemberVO">
        <![CDATA[
            SELECT *
            FROM "member_table"
            WHERE MEM_ID = #{mem_id}
        ]]>
    </select>
</mapper>